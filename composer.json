{
  "name": "ttek/tk8base",
  "type": "project",
  "description": "A base example site using the tropotek tk v8 libs.",
  "keywords": ["tropotek"],
  "time": "2022-10-17",
  "license": "MIT",
  "minimum-stability": "dev",
  "authors": [
    {
      "name": "Michael Mifsud",
      "homepage": "http://www.tropotek.com/"
    }
  ],
  "autoload": {
    "psr-0": {
      "App\\": "src/"
    },
    "exclude-from-classmap": [
      "/tests/"
    ]
  },
  "scripts": {
    "post-install-cmd": [
      "Tk\\Composer\\Installer::postInstall"
    ],
    "post-update-cmd": [
      "Tk\\Composer\\Installer::postUpdate"
    ]
  },
  "require": {
    "php": "^8.1",
    "ext-curl": "*",
    "ext-pdo": "*",
    "ext-dom": "*",
    "ext-libxml": "*",
    "ttek/tk-framework": "~8.0.0",
    "ttek/tk-auth": "~8.0.0",
    "ttek/tk-domtemplate": "~8.0.0",
    "ttek/tk-table": "~8.0.0",
    "ttek/tk-form": "~8.0.0",
    "ttek/tk-mail": "~8.0.0"
  },
  "require-dev": {
    "phpunit/phpunit": "9.5.x-dev",
    "composer/composer": "^2.0"
  },
  "config": {
    "allow-plugins": {
      "ttek/tk-installers": true
    }
  }
}
